<h1 class="title">Explore Destinations</h1>
<div class="columns is-multiline">
    {{#each destinations}}
    <div class="column is-one-quarter">
        <div class="card">
            <div class="card-content">
                <p class="title">{{city}}, {{country}}</p>
                <p class="subtitle">{{description}}</p>
            </div>
        </div>
    </div>
    {{/each}}
</div>
